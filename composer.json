{
  "name":"edosenseidotcom/lagoon",
  "type":"project",
  "license":"MIT",
  "require":{
    "php": ">=8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "amazeeio/drupal_integrations": "^0.3",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/advagg": "^4.1",
    "drupal/allowed_formats": "^1",
    "drupal/block_visibility_conditions": "^1",
    "drupal/commerce": "^2.20",
    "drupal/commerce_stripe": "^1.0@rc",
    "drupal/consumer_image_styles": "^4.0",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-recommended": "^9",
    "drupal/critical_css": "^1.16",
    "drupal/csp": "^1",
    "drupal/drulma": "1.x-dev#3e5788e6e97d0cdced75ea621049d647c7180c6e",
    "drupal/easy_breadcrumb": "^2",
    "drupal/email_registration": "^1",
    "drupal/feeds": "^3.0@beta",
    "drupal/feeds_tamper": "^2.0@beta",
    "drupal/field_group": "^3.1",
    "drupal/filefield_paths": "^1.0@beta",
    "drupal/firebase": "^3",
    "drupal/flag": "^4.0@beta",
    "drupal/gin": "^3.0@alpha",
    "drupal/gin_toolbar": "^1.0@beta",
    "drupal/google_analytics": "^4",
    "drupal/honeypot": "^2.0",
    "drupal/image_effects": "^3.0",
    "drupal/imagemagick": "^3.1",
    "drupal/job_scheduler": "^3",
    "drupal/json_field": "^1.0@rc",
    "drupal/jsonapi_extras": "^3",
    "drupal/jsonapi_include": "^1",
    "drupal/jsonapi_resources": "^1.0@beta",
    "drupal/lagoon_logs": "^2",
    "drupal/markdown": "^3.0@rc",
    "drupal/media_entity_file_replace": "^1.0",
    "drupal/metatag": "^1.13",
    "drupal/minifyhtml": "^1.9",
    "drupal/pathauto": "^1.8",
    "drupal/pwa": "^1",
    "drupal/redirect": "^1.5",
    "drupal/redis": "^1",
    "drupal/rename_admin_paths": "^2",
    "drupal/resave_all_nodes": "^1.0@alpha",
    "drupal/rrssb": "^2.2",
    "drupal/schema_metatag": "^2.0",
    "drupal/seckit": "^2.0",
    "drupal/simple_sitemap": "^4.0@alpha",
    "drupal/simple_styleguide": "^1.8",
    "drupal/symfony_mailer": "^1@alpha",
    "drupal/tamper": "^1.0@alpha",
    "drupal/token_filter": "^1.2",
    "drupal/views_bulk_operations": "^4.0",
    "drupal/webform": "6.1.x-dev#37efe8f287031992368a489563528aeb85fafb24",
    "drupal/webform_rest": "^4",
    "drupal/weight": "^3",
    "drush-ops/behat-drush-endpoint": "^9.4.1",
    "drush/drush": "^11",
    "league/commonmark": "^1.4",
    "oomphinc/composer-installers-extender": "^2",
    "platformsh/config-reader": "^2",
    "rexxars/html-validator": "^2.2.0",
    "wikimedia/composer-merge-plugin": "^2"
  },
  "conflict":{
    "drupal/drupal":"*"
  },
  "require-dev":{
    "behat/mink-selenium2-driver":"1.5.0",
    "dealerdirect/phpcodesniffer-composer-installer":"^0.7",
    "drupal/coder":"^8.3",
    "drupal/config_override_warn":"^1.3",
    "drupal/commerce_paypal":"^1.0@beta",
    "drupal/composer_deploy":"^1",
    "drupal/devel":"^4.0",
    "drupal/devel_kint_extras":"^1",
    "drupal/diff":"^1.0",
    "drupal/drulma_companion":"^2",
    "drupal/drupal-driver":"^2.1",
    "drupal/drupal-extension":"^4.1",
    "drupal/entity_usage":"^2.0@beta",
    "drupal/examples":"^3",
    "drupal/field_tools":"^1@alpha",
    "drupal/firebase_authentication":"^1@alpha",
    "drupal/getjwtonlogin":"^2",
    "drupal/jsonapi_explorer":"^1",
    "drupal/jsonapi_user_resources":"^1.0@alpha",
    "drupal/jwt":"^1.0@beta",
    "drupal/oauth2_server":"^2@beta",
    "drupal/openid_connect":"^1",
    "drupal/nocurrent_pass":"^1",
    "drupal/queue_ui":"^2",
    "drupal/restui":"^1",
    "drupal/tome":"^1.4",
    "drupal/upgrade_status":"^3",
    "emuse/behat-html-formatter":"^0.2",
    "friends-of-behat/service-container-extension":"^1",
    "genesis/behat-fail-aid":"^3.5",
    "jcalderonzumba/gastonjs":"^1.0.2",
    "jcalderonzumba/mink-phantomjs-driver":"^0.3.1",
    "kint-php/kint":"^3.3",
    "kint-php/kint-twig":"^2.0",
    "mikey179/vfsstream":"^1.2",
    "squizlabs/php_codesniffer":"^3.4.0",
    "symfony/css-selector":"^2.8"
  },
  "config":{
    "allow-plugins":{
      "composer/installers":true,
      "cweagans/composer-patches":true,
      "dealerdirect/phpcodesniffer-composer-installer":true,
      "drupal/core-composer-scaffold":true,
      "drupal/core-project-message":true,
      "oomphinc/composer-installers-extender":true,
      "wikimedia/composer-merge-plugin":true,
      "zaporylie/composer-drupal-optimizations":true
    },
    "optimize-autoloader":true,
    "platform":{
      "php":"8.0"
    },
    "preferred-install":"dist",
    "sort-packages":true
  },
  "extra":{
    "composer-exit-on-patch-failure":true,
    "enable-patching":true,
    "drupal-scaffold":{
      "allowed-packages":[
        "amazeeio/drupal_integrations"
      ],
      "file-mapping":{
        "[web-root]/sites/default/all.settings.php":"assets/all.settings.php"
      },
      "locations":{
        "web-root":"web/"
      }
    },
    "installer-paths":{
      "drush/Commands/contrib/{$name}":[
        "type:drupal-drush"
      ],
      "web/core":[
        "type:drupal-core"
      ],
      "web/libraries/{$name}":[
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}":[
        "type:drupal-module"
      ],
      "web/private/scripts/quicksilver/{$name}/":[
        "type:quicksilver-script"
      ],
      "web/profiles/contrib/{$name}":[
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}":[
        "type:drupal-theme"
      ]
    },
    "installer-types":[
      "bower-asset",
      "npm-asset"
    ],
    "merge-plugin":{
      "include":[
        "web/modules/contrib/webform/composer.libraries.json"
      ]
    },
    "patches":{
      "drupal/address":{
        "Ignore whitespace in zip codes https://www.drupal.org/project/address/issues/2994943":"patches/2994943-9-strip-whitespace.patch"
      },
      "drupal/commerce":{
        "allow order types to have no carts":"https://www.drupal.org/files/issues/2020-10-05/2810723-130.patch"
      },
      "drupal/commerce_stripe":{
        "fix 1112 errors https://www.drupal.org/project/commerce_stripe/issues/3177168":"https://www.drupal.org/files/issues/2020-10-16/3177168-2_commerce_stripe-fix_address_errors.patch",
        "skip review step by autosubmit":"https://www.drupal.org/files/issues/2020-09-16/commerce_stripe-3171411-2.patch"
      },
      "drupal/core":{
        "Allow roles visibility condition to be negated":"https://www.drupal.org/files/issues/2018-07-19/enable-block-visibility-user-role-negation-2986958-2.patch",
        "Csrf token gets required even when using OAuth bearer https://www.drupal.org/project/drupal/issues/3055260":"https://www.drupal.org/files/issues/2021-01-04/3055260-add-additional-condition-to-csrf-check-32.patch",
        "Disable update disabled warning":"https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch",
        "Fix views import error":"https://www.drupal.org/files/issues/2020-09-03/3007424-108.patch",
        "Negate form value fix for google_tag":"https://www.drupal.org/files/issues/2020-03-23/3114467-8.patch",
        "REST user registration broken https://www.drupal.org/project/drupal/issues/3055807":"https://www.drupal.org/files/issues/2022-01-10/drupal-3055807-29-users-registered-via-rest-get-blocked.patch",
        "Remove experimental modules/themes warning https://www.drupal.org/project/drupal/issues/2880374":"https://www.drupal.org/files/issues/2021-11-12/2880374-44.patch",
        "Remove security warning that only applies to apache https://www.drupal.org/project/drupal/issues/3117665":"https://www.drupal.org/files/issues/2020-03-04/3117665-non_apache_servers_4.patch",
        "Restore translation string deletion":"https://www.drupal.org/files/issues/2020-09-21/2503893-36.patch",
        "Symlinks can't be used in files directory https://www.drupal.org/project/drupal/issues/1008402":"https://www.drupal.org/files/issues/2021-07-29/1008402-148.patch",
        "Remove layout builder's dependency on field_ui https://www.drupal.org/project/drupal/issues/2935999":"https://git.drupalcode.org/project/drupal/-/merge_requests/711.diff"
      },
      "drupal/csp":{
        "Auto sources should use full file path https://www.drupal.org/project/csp/issues/3263346":"https://www.drupal.org/files/issues/2022-02-09/3263346-auto-sources-should.patch"
      },
      "drupal/email_registration":{
        "behat":"https://www.drupal.org/files/issues/2020-01-31/2843505-27.patch",
        "enable login via RPC https://www.drupal.org/project/email_registration/issues/2856542":"https://www.drupal.org/files/issues/2021-06-11/2856542-13.patch"
      },
      "drupal/feeds":{
        "https://www.drupal.org/project/feeds/issues/3260298 provide an indicator when same field mapped twice":"https://www.drupal.org/files/issues/2022-02-04/3260298-16.patch"
      },
      "drupal/filefield_paths":{
        "add origname to file table - fixed in next release (rc6?)":"https://www.drupal.org/files/issues/2020-03-13/3119771-2.patch",
        "fix call to thirdpartysettings https://www.drupal.org/project/filefield_paths/issues/2718783":"https://www.drupal.org/files/issues/2020-04-27/2718783-38-reroll.patch"
      },
      "drupal/flag":{
        "https://www.drupal.org/project/flag/issues/3091824 improve JSON:API":"https://git.drupalcode.org/project/flag/-/merge_requests/15.patch"
      },
      "drupal/google_analytics":{
        "remove jquery dependency https://www.drupal.org/project/google_analytics/issues/2798731":"https://www.drupal.org/files/issues/2022-02-27/2798731-25-google_analytics-remove_jquery.patch",
        "fix php 8.1 error https://www.drupal.org/project/google_analytics/issues/3258588":"https://git.drupalcode.org/issue/google_analytics-3258588/-/merge_requests/1.patch"
      },
      "drupal/honeypot":{
        "add behat context":"https://www.drupal.org/files/issues/2020-09-09/3059040-31.patch"
      },
      "drupal/json_field":{
        "add feeds support":"https://www.drupal.org/files/issues/2021-07-07/3222366-2--json_field--add_feeds_support.patch"
      },
      "drupal/jsonapi_include":{
        "add compatibility with jsonapi_resource":"https://www.drupal.org/files/issues/2020-09-24/jsonapi_resources-3172870-2.patch"
      },
      "drupal/jwt":{
        "make exp time configurable, https://www.drupal.org/project/jwt/issues/2782571":"https://www.drupal.org/files/issues/2019-09-24/2782571-11.patch"
      },
      "drupal/markdown":{
        "add role attribute support":"https://www.drupal.org/files/issues/2022-02-18/3265161-2_markdown_add-role-attribute.patch"
      },
      "drupal/metatag":{
        "feeds integration https://www.drupal.org/project/metatag/issues/3011202":"https://www.drupal.org/files/issues/2020-09-29/feeds-integration-3011202-30_1.patch"
      },
      "drupal/pathauto":{
        "prevent pathauto from duping itself https://www.drupal.org/project/pathauto/issues/3107144":"https://www.drupal.org/files/issues/2020-01-17/pathauto-3107144-2.patch"
      },
      "drupal/webform_rest":{
        "Do not require REST UI https://www.drupal.org/project/webform_rest/issues/3205459":"https://www.drupal.org/files/issues/2021-03-24/3205459-webform_rest-no-restui-dep-4.patch"
      }
    }
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://packages.drupal.org/8",
      "canonical":false
    }
  ],
  "minimum-stability":"beta",
  "prefer-stable":true,
  "scripts":{
    "code-sniff":[
      "./vendor/bin/phpcs --standard=Drupal,DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor,*.min.* ./web/modules/custom ./web/themes/custom"
    ],
    "lint":"find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;"
  }
}
